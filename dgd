Resources:
  MySNSTopic:
    Type: AWS::SNS::Topic
    Properties:
      TopicName: !Ref TopicName
      Subscription:
        - Endpoint: !Ref EmailAddress
          Protocol: email

Outputs:
  TopicArn:
    Description: The ARN of the created SNS topic
    Value: !Ref MySNSTopic

  TopicName: 
    Description: The name of the created SNS topic
    Value: !Ref TopicName

{
    {
        "EmailAddress": "Nitin.Patil@exlservice.com"
    },
    {
        "Stacktags": "tag1=value1 tag2=value2"
    },
    {
        "TopicName": "NitinTestTopic"
    }
}
